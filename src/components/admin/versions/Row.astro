---
interface Props {
	data: {
        name: string;
        checksums_enabled: number;
        supported: number;
        flags: number;
    };
}

const { data } = Astro.props;
---
<tr class="versions-entry">
    <td>{data.name}</td>
    <td>
        {data.checksums_enabled === 0 ? <span class="badge badge-error">Disabled</span> : <span class="badge badge-success">Enabled</span>}
    </td>
    <td>
        {data.supported === 0 ? <span class="badge badge-warning">Legacy</span> : <span class="badge badge-success">Supported</span>}
    </td>
    <td>{data.flags  === 0 ? 'NONE' : 'VF_REAL_TYPES'}</td>
    <td>
        <button class="btn-primary">
